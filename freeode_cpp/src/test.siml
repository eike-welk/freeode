MODEL mo2

PARAMETER
mu_max DEFAULT 0.32;
Ks     DEFAULT 0.1;
Yxs    DEFAULT 0.5;

VARIABLE
mu;
X INITIAL 0.2; # Biomass
S INITIAL 25;  # Sugar

EQUATION
mu := mu_max * S/(S+Ks);
$X := mu*X;
$S := -1/Yxs*mu*X;

END


PROCESS pr1

PARAMETER
mu_max;

UNIT
reactor AS mo2;
#reactor2 AS mo2;

SET
mu_max := 0.5;

#EQUATION

INITIAL
reactor.X := 0.1;

SOLUTIONPARAMETERS
ReportingInterval :=1; #[h]
SimulationTime    :=20; #[h]

#SCHEDULE
#CONTINUE FOR 1000

END
